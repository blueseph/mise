{
  "name": "@mise/core",
  "version": "3.5.0",
  "description": "a small, no-frills library to build complex client-side applications and easily manage state",
  "main": "dist/mise.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/blueseph/mise.git"
  },
  "directories": {
    "example": "example"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "test": "jest --coverage --verbose",
    "test:watch": "jest --coverage --verbose --watch",
    "build": "webpack --mode=production",
    "build:unoptimized": "webpack",
    "lint": "eslint ./src/",
    "lint:fix": "eslint --fix ./src/",
    "start": "npm run lint && npm run test && npm run build",
    "prepublishOnly": "npm start"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/utils.js"
    ],
    "testURL": "http://localhost"
  },
  "bundlesize": [
    {
      "path": "./dist/mise.js"
    }
  ],
  "author": "dan b.",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-transform-react-jsx": "^7.0.0",
    "@babel/preset-env": "7.1.0",
    "@mise/test": "^1.0.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "8.2.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "eslint": "^5.8.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^29.3.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  }
}
